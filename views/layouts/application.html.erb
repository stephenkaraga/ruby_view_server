<html>
<%
def link_to(name, url)
  return "<a href='#{url}'>#{name}</a>"
end
%>
  <body>
    <h1>You're using a layout</h1>
    <p>
      <h3> I'm a header</h3>
      <p><%= link_to('This is a link to google', 'http://www.google.com') %></p>
      <a href='me.html'>Me</a>

    </p>
    <hr />
    <div>
      <%= yield %>
    </div>
    <hr />
    <div>
      <h3> I'm a footer</h3>
      <p><%= link_to("This is a link to CNN", 'http://www.cnn.com') %></p>
    </div>
  </body>
</html>